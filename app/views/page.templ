package views 

templ Hello(name string) {
    <html lang="en">
        <head>
            <title>Home Page</title>
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <link rel="stylesheet" href="/styles/styles.css">
        </head>
        <body class="font-bold underline text-black">
            Hello, { name }
        </body>
    </html>
}
